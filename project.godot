config_version=5

_global_script_classes=[{
"base": "Control",
"class": &"InfoBox",
"language": &"GDScript",
"path": "res://addons/node_position_debugger/position_info_box.gd"
}, {
"base": "Node2D",
"class": &"Tracker2D",
"language": &"GDScript",
"path": "res://addons/node_position_debugger/tracker_2d.gd"
}]
_global_script_class_icons={
"InfoBox": "",
"Tracker2D": ""
}

[application]

config/name="NodePositionDebugger"
run/main_scene="res://addons/node_position_debugger/test_scene.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

PositionDebuggerOverlay="*res://addons/node_position_debugger/position_info_overlay.tscn"

[editor_plugins]

enabled=PackedStringArray("res://addons/node_position_debugger/plugin.cfg")
